#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
int num,sum=0,count=0;
    scanf("%d",&num);
    int temp=num;
    while(temp!=0){
        temp =temp/10;
        count++;
    }
    temp=num;
    while(temp!=0){
        int Lastdigit =temp%10;
        sum=sum + pow(Lastdigit,count);
        temp=temp/10;
    }
    if(num==sum)
        printf("Armstrong");
        else
            printf("Not Armstrong");
    return 0;
}
