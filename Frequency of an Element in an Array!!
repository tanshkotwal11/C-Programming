#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N,i,ele,count=0;
    scanf("%d",&N);
    int arr[N];
    for(i=0;i<N;i++){
        scanf("%d",&arr[i]);
    
    }
    scanf("%d",&ele);
          for(i=0;i<N;i++){
              if(arr[i]==ele)
                  count++;
          }
          printf("%d",count);
         return 0;
}
