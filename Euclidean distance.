#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int x1, y1, x2, y2;
    float distance;
    scanf("%d %d", &x1, &y1);
    scanf("%d %d", &x2, &y2);
    distance = sqrt((x2 - x1) * (x2 - x1) + (y2 - y1) * (y2 - y1));
    printf("%.2f\n", x1, y1, x2, y2, distance);
    return 0;
}
     
